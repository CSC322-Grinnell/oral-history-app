<% provide(:title, 'Interview Submission') %>
<h1>Interview Submission</h1>

<div class="row">
<div class="col-md-6 col-md-offset-3">
  <%= form_with do |f| %>
  <%= f.label :intervieweeName, "Interviewee:" %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :interviewerName, "Interviewer:" %>
  <%= f.text_field :interviewer %>
  <br>
  <%= f.label :date, "Date of Interview:" %>
  <%= f.text_field :date %>
  <br>
  <%= f.label :location, "Location of Interview" %>
  <%= f.text_field :location %>
  <br>
  <%= f.label :audioFile, "Audio File:" %>
  <%= f.text_field :audio_link %>
  <br>
  <%= f.label :summary, "Text Summary:" %>
  <%= f.text_field :summary %>
  <br>
  <%= f.label :transcription, "Interview Transcription:" %>
  <%= f.text_field :transcript %>
  <br>
  <%= f.submit "Submit Interview" %>
  <% end %>
  </div>
</div>
  
  
